{
  "name": "@jspsych/metadata-cli",
  "version": "1.0.0",
  "main": "cli.js",
  "type": "module",
  "scripts": {
    "data": "node dist/cli.js ../../dev/data",
    "data:abs": "node dist/cli.js ~/Documents/work/ursi2024/metadata/dev/data ~/Documents/work/ursi2024/metadata/dev/options/metadata-options.json",
    "data:rec": "node dist/cli.js ../../dev/data-rec",
    "options": "node dist/cli.js ../../dev/data ../../dev/options/metadata-options.json",
    "build": "esbuild src/cli.ts --bundle --format=esm --platform=node --outfile=dist/cli.js",
    "build:data": "esbuild src/data.ts --packages=external --format=esm --platform=node --outfile=dist/data.js",
    "build:cli": "npm run build:data && esbuild src/index.ts --packages=external --format=esm --platform=node --outfile=dist/index.js",
    "cli": "node dist/index.js"
  },
  "dependencies": {
    "@inquirer/prompts": "^5.1.2",
    "fuzzy": "^0.1.3",
    "inquirer": "^10.0.1",
    "inquirer-autocomplete-prompt": "^3.0.1",
    "metadata": "file:../metadata/"
  }
}
